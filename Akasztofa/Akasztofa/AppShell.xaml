<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Akasztofa.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Akasztofa"
    Shell.FlyoutBehavior="Flyout"
    Title="Akasztofa">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
    Title="GamePage"
    ContentTemplate="{DataTemplate local:GamePage}"
    Route="GamePage" />
    <ShellContent
    Title="ReportPage"
    ContentTemplate="{DataTemplate local:ReportPage}"
    Route="ReportPage" />

</Shell>
